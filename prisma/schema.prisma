generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model user {
  id         String   @id @default(uuid())
  username   String   @unique
  password   String
  role       String
  created_at DateTime @default(now())
}

model kelas {
  id          String   @id @default(uuid())
  class_name  String
  description String
  schedule    String
  price       String
  class_items String[]
  created_at  DateTime @default(now())
}

model contact_information {
  id               String   @id @default(uuid())
  address          String
  phone            String
  email            String
  operational_time String[]
  created_at       DateTime @default(now())
}

model image {
  id              String   @id @default(uuid())
  image_public_id String
  url             String
  created_at      DateTime @default(now())
}
